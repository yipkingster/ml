      "source": [
        "import jax\n",
        "import jax.numpy as jnp\n",
        "\n",
        "arr = jnp.full([3], 2.0)\n",
        "# Default behavior: last element\n",
        "print(arr.at[3].get())\n",
        "print(arr[4])\n",
        "\n",
        "# Change behavior to controlled fill.\n",
        "print(arr.at[5].get(mode='fill', fill_value=jnp.nan))"
      ]
    }
